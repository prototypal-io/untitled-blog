<article>
  <header>
    <h1>{{model.title}}</h1>

    <p>
      Posted by {{model.author.fullName}} on
      <time datetime="{{moment-format post.publishedAt "YYYY-MM-DD"}}">
        {{moment-format post.publishedAt "MMMM D, YYYY"}}
      </time>
    </p>
  </header>

  {{{model.body}}}
</article>
